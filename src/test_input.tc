# require x >= 1
# require y >= 0
# x <= 2 && y <= 2 => _ <= 12
fn foo(x: int, y: int) -> int {
	bar(x, y, 3)
}

# require y >= 0
# require z >= 0
# x <= 2 => _ <= 3*y+2*z
fn bar(x: int, y: int, z: int) -> int {
	# lemma x <= x ==> x*(y+z)+y <= 3*y + 2*z
	x * (y + z) + y
}

fn main() {
	let x = 3;
	# lemma x >= 4
	print foo(x, 2);
}

fn t(my_struct: { hello: int, world: {x: int, y: int, z: bool} }) -> {a: int, b: bool, c: int} {
	{
		a: 2,
		b: my_struct.world.z,
		c: my_struct.hello
	}
}

fn u(ooo: {a: int, b: bool, c: int}) -> int {
	ooo.a
}

fn v(z: bool) -> { hello: int, world: {x: int, y: int, z: bool} } {
	{
		hello: 5,
		world: {
			x: 6,
			y: 7,
			z: z,
		}
	}
}

fn w(x: bool) -> int {
	u(t(v(x)))
}

